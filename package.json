{
  "name": "mcp-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron-dist/main.js",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "tsc -b && vite build && tsc -p tsconfig.electron.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "echo 'Cleaning build artifacts and caches...' && rm -rf dist electron-dist .vite .cache .parcel-cache logs && find . -name '*.log*' -type f -delete && rm -rf node_modules/.tmp node_modules/.vite && find . -type d \( -name Crashpad -o -name crashpad_database -o -name crashpad \) -prune -exec rm -rf {} +",
    "clean:deep": "pnpm clean && rm -rf node_modules"
  },
  "engines": {
    "node": ">=22.17.1"
  },
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "electron": "^37.2.6",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "wait-on": "^8.0.4"
  }
}
